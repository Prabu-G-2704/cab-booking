<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>cabServlet</servlet-name>
    <servlet-class>com.cabbooking.BookCab</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>empReportServlet</servlet-name>
    <servlet-class>com.cabbooking.EmployeeReport</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>orgReportServlet</servlet-name>
    <servlet-class>com.cabbooking.OrganisationReport</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>agencyReport</servlet-name>
    <servlet-class>com.cabbooking.AgencyReport</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>cabServlet</servlet-name>
    <url-pattern>/cab</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>empReportServlet</servlet-name>
    <url-pattern>/employee/report</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>orgReportServlet</servlet-name>
    <url-pattern>/organisation/report</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>agencyReport</servlet-name>
    <url-pattern>/agency/report</url-pattern>
  </servlet-mapping>

</web-app>
